local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("2019 Booga Booga [REBORN] | fal#0001", "BloodTheme")
local Main = Window:NewTab("Booga")
local Visual = Window:NewTab("Visuals")
local Changelog = Window:NewTab("Changelog")
local Settings = Window:NewTab("Settings")
local ChangelogSection = Changelog:NewSection("Changelog")
local MainSection = Main:NewSection("Stuff")
local VisualSection = Visual:NewSection("Visuals")
local SettingSection = Settings:NewSection("Settings")
MainSection:NewLabel("waterbypass = loopws 16 on IY")
ChangelogSection:NewLabel("10-6-22 (Added Gold/Crystal Esp)")
MainSection:NewButton("Infinite Yield", "ButtonInfo", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
end)
MainSection:NewButton("Unnamed Esp", "ButtonInfo", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/ic3w0lf22/Unnamed-ESP/master/UnnamedESP.lua'))()
end)
VisualSection:NewButton("Crystal Lode Esp", "ButtonInfo", function()
    for i,v in pairs(game.Workspace:GetDescendants()) do -- grabs everything from workspace
        if v.ClassName == 'UnionOperation' and v.Parent.Name == 'Crystal Lode' then -- checks if it has a handle and if its a touchtransmitter
            local BillboardGui = Instance.new('BillboardGui') -- Makes Billboardgui
            local TextLabel = Instance.new('TextLabel') -- makes text label
            
            BillboardGui.Parent = v.Parent -- what the billboardgui goes into
            BillboardGui.AlwaysOnTop = true -- if its on top or not
            BillboardGui.Size = UDim2.new(0, 50, 0, 50) -- size of it
            BillboardGui.StudsOffset = Vector3.new(0,2,0)
            
            TextLabel.Parent = BillboardGui -- putting textlabel into billboardgui
            TextLabel.BackgroundColor3 = Color3.new(1,1,1) -- color
            TextLabel.BackgroundTransparency = 1 -- transparency
            TextLabel.Size = UDim2.new(1, 0, 1, 0) -- size
            TextLabel.Text = "Crystal Node" -- what the label says
            TextLabel.TextColor3 = Color3.new(128, 187, 219) -- color
            TextLabel.TextScaled = false -- if the text is scaled or not
            TextLabel.TextStrokeTransparency = true
        end
    end
end)
VisualSection:NewButton("Gold Node Esp", "ButtonInfo", function()
    for i,v in pairs(game.Workspace:GetDescendants()) do -- grabs everything from workspace
        if v.ClassName == 'UnionOperation' and v.Parent.Name == 'Gold Node' then -- checks if it has a handle and if its a touchtransmitter
            local BillboardGui = Instance.new('BillboardGui') -- Makes Billboardgui
            local TextLabel = Instance.new('TextLabel') -- makes text label
            
            BillboardGui.Parent = v.Parent -- what the billboardgui goes into
            BillboardGui.AlwaysOnTop = true -- if its on top or not
            BillboardGui.Size = UDim2.new(0, 50, 0, 50) -- size of it
            BillboardGui.StudsOffset = Vector3.new(0,2,0)
            
            TextLabel.Parent = BillboardGui -- putting textlabel into billboardgui
            TextLabel.BackgroundColor3 = Color3.new(1,1,1) -- color
            TextLabel.BackgroundTransparency = 1 -- transparency
            TextLabel.Size = UDim2.new(1, 0, 1, 0) -- size
            TextLabel.Text = "Gold Node" -- what the label says
            TextLabel.TextColor3 = Color3.new(255, 195, 0) -- color
            TextLabel.TextScaled = false -- if the text is scaled or not
            TextLabel.TextStrokeTransparency = true 
        end
    end
end)
SettingSection:NewKeybind("Hide Gui", ".", Enum.KeyCode.V, function()
	Library:ToggleUI()
end)
